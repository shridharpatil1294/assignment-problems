   "source": [
    "items = (8, 2, 3, 0, 7)\n",
    "items_in_str = []\n",
    "for i in items:\n",
    "    items_in_str.append(str(i))\n",
    "lhs = '+'.join(items_in_str)\n",
    "rhs = sum(items)\n",
    "lhs+'='+str(rhs)"
